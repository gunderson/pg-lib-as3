package pg.anim{	public class ColorTween{		private function colorFade(f_startColor:int, f_endColor:int, f_position:Number):int{			var r:int			var g:int			var b:int			f_position = (f_position > 1) ? 1 : f_position;			f_position = (f_position < 0) ? 0 : f_position;			r = (f_startColor >> 16) + ((f_endColor >> 16 - (f_startColor >> 16)) * f_position);			g = ((f_startColor >> 8) & 0xff) + ((((f_endColor >> 8) & 0xff) - ((f_startColor >> 8) & 0xff)) * f_position);			b = (f_startColor & 0xff) + (((f_endColor & 0xff) - (f_startColor & 0xff)) * f_position);			return (r << 16 | g << 8 | b)		}	}}